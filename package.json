{
  "engines": {
    "node": ">=0.10.32"
  },
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "a starterkit of frontend website",
  "main": "app/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf build",
    "build":"npm run clean && cross-env NODE_ENV=development webpack --colors --verbose --display-error-details --config webpack.config.js",
    "watch": "webpack-dev-server --hot --inline --config webpack.config.js",
    "serve": "npm run clean && npm run watch"
  },
  "keywords": [
    "webpack",
    "frontend",
    "js",
    "css",
    "starter",
    "template"
  ],
  "author": "haozhigang@outlook.com",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-stage-0": "6.x",
    "babel-preset-latest": "^6.x",
    "babel-runtime": "6.x",
    "cross-env": "^2.0.0",
    "css-loader": "^0.25.0",
    "eslint": "latest",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "2.24.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "open-browser-webpack-plugin": "0.0.2",
    "favicons-webpack-plugin":"latest",
    "postcss-loader": "^1.1.1",
    "postcss-import":"latest",
    "postcss-cssnext":"latest",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "autoprefixer": "^6.0.3",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  }
}
